<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="TOP_RIGHT"
         maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308"
         prefWidth="400.0"
         type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1"
         style="-fx-background-color: rgba(255, 255, 168, 0.8); -fx-border-color: #b3af76; -fx-border-width: 2; -fx-border-radius: 10; -fx-padding: 10;">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity"
               style="-fx-background-color: transparent;"/>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
            <clip>
                <Circle radius="49.5" centerX="49.5" centerY="49.5" />
            </clip>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
